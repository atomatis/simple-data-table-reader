{
    "name": "atomatis/simple-data-table-reader",
    "description": "For easily parse basic data table files (xlsx, csv, ...).",
    "type": "library",
    "keywords": ["csv", "xlsx", "data table", "reader", "file"],
    "homepage": "https://github.com/atomatis/simple-data-table-reader",
    "require": {
        "php": ">=7.2.5",
        "phpoffice/phpspreadsheet": "^1.15",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "alexandre tomatis",
            "email": "alexandre.tomatis@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SimpleDataTableReader\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleDataTableReader\\Tests\\": "tests/"
        }
    }
}
